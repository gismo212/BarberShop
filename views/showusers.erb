<h2>Users Information</h2>

<table class="table table-striped">
	<thead>
		<tr>
			<th>Name</th>
			<th>Phone</th>
			<th>Datatime</th>
			<th>Barber</th>
			<th>Color </th>
		</tr>
	</thead>

	<tbody>
		<% @results.each do |row| %>
			<tr>
				<td><%=row['username'] %></td>
				<td><%=row['phone']%></td>
				<td><%=row['datestamp']%></td>
				<td><%=row['barber']%></td>
				<td><%=row['color']%></td>
			</tr>

		<% end %>
	</tbody>
</table>


